    "Opportunity": {
      "description": "Represents an opportunity (potential revenue)",
      "api_endpoint": "/services/data/v59.0/sobjects/Opportunity",
      "fields": {
        "Id": {
          "type": "string",
          "description": "Unique Salesforce ID",
          "required": true
